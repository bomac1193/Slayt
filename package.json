{
  "name": "slayt",
  "version": "2.0.0",
  "description": "AI-powered social media content planner and scheduler with grid preview",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "mini:dev": "npm run dev & npm run dev --prefix client",
    "mini:server": "npm run dev",
    "mini:client": "npm run dev --prefix client",
    "mini:build": "npm run build --prefix client",
    "mini:publish": "npm run build --prefix client && rm -rf public/alchemy && cp -R client/dist public/alchemy",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "social-media",
    "scheduler",
    "content-planner",
    "instagram",
    "tiktok",
    "ai",
    "grid-preview"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@google-cloud/vision": "^4.1.0",
    "@tensorflow/tfjs-node": "^4.11.0",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^2.0.9",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "openai": "^4.20.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-instagram": "^1.0.0",
    "passport-oauth2": "^1.7.0",
    "replicate": "^1.4.0",
    "sharp": "^0.33.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
